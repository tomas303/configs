[DISK]
command=df -h / | awk '/\//{ printf(" Disk %4s/%s \n", $4, $2) }'
interval=2
color=#C9E3DB

[MEM]
command=free -h | awk '/Mem:/ { printf(" Mem %5s/%s \n", $3, $2) }'
interval=1
color=#FEC925

[TIME_DATE]
command=date +" %a, %d %b - %H:%M:%S"
interval=1

[keymap]
command=~/.config/i3scripts/key_layout
interval=1

[battery]
command=~/.config/i3scripts/battery
interval=30

